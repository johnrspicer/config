! ! █▓▒░ scorcerer colors
*.foreground:   #c2c2b0
*.background:   #222222
*.cursorColor:  #c2c2b0
! black
*.color0:       #111111
*.color8:       #181818
! red
*.color1:       #aa4450
*.color9:       #ff6a6a
! green
*.color2:       #719611
*.color10:      #b1d631
! yellow
*.color3:       #ff9800
*.color11:      #87875f
! blue
*.color4:       #6688aa
*.color12:      #90b0d1
! magenta
*.color5:       #8f6f8f
*.color13:      #8181a6
! cyan
*.color6:       #528b8b
*.color14:      #87ceeb
! white
*.color7:       #d3d3d3
*.color15:      #c1cdc1
! stylized
*.colorIT:      #719611
*.colorBD:      #d3d3d3
*.colorUL:      #528b8b

! cursor size
Xcursor.size: 2
URxvt*font: -xos4-terminus-medium-r-*-*-18-*-*-*-*-*-iso10646-*
URxvt*iso14755: false
URxvt*scrollBar: false
URxvt*keysym.Control-Meta-c: builtin-string:
URxvt*keysym.Control-Meta-v: builtin-string:
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt*keysym.Shift-Control-V: eval:paste_clipboard
